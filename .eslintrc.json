{
  "env": {
    "es6": true,
    "node": true
  },
  "root": true,
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
  ],
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "rules": {
  },
  "overrides": [
    {
      "files": [
        "**/*.ts",
        "**/*.tsx"
      ],
      "rules": {
        "no-redeclare": "off",
        "no-undef": "off",
        "no-unused-vars": "off"
      }
    }
  ]
}